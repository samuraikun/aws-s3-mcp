{
  "tasks": {
    "start": "deno run --allow-net --allow-env --allow-read --allow-npm src/index.ts",
    "test": "deno test --allow-net --allow-env --allow-read --allow-npm",
    "build": "deno compile --allow-net --allow-env --allow-read --allow-npm --output dist/aws-s3-mcp src/index.ts"
  },
  "imports": {
    "std/": "https://deno.land/std@0.224.0/",
    "@aws-sdk/client-s3": "npm:@aws-sdk/client-s3@3.758.0",
    "@modelcontextprotocol/sdk": "npm:@modelcontextprotocol/sdk@1.9.0",
    "pdf-parse": "npm:pdf-parse@1.1.1",
    "ts-pattern": "npm:ts-pattern@5.6.2",
    "zod": "npm:zod@3.22.4",
    "zod-to-json-schema": "npm:zod-to-json-schema@3.24.3"
  },
  "compilerOptions": {
    "lib": ["deno.window", "deno.ns"],
    "strict": true
  },
  "nodeModulesDir": "auto"
}
